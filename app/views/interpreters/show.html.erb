 <h1><%= @interpreter.name %></h1>
 <h2><%= truncate(@interpreter.bio, :length => 40, :separator => ' ')%></h2>
 <p><%= @interpreter.location %></p>
 <p><%= @interpreter.price %></p>
 <p><%= @interpreter.years_of_experience %></h1>
   <p><% @interpreter.reviews.all.each do |review|%></p>
   <ul>
      <p> Overall Performance: <%= review.overall_performance%> </p>
        <p> Language Skill: <%= review.language_skill%> |
        Friendliness: <%= review.friendliness%>|
        Professionalism: <%= review.professionalism%> |
        Punctuality: <%= review.punctuality%></p>
      <p> Description: <%= review.content%></p>
    </ul>
    <% end %>
    <p><%= render 'layouts/booking' %></p>
