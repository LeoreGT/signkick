<div class="text-center padded-div-bottom">
  <%= simple_form_for([interpreter, language_skill], remote: true) do |f| %>
  <%= f.input :language_id, collection: Language.all.order('name ASC') %>
  <%= f.button :submit, 'Add Language!', :class => "add_language" %>
  <% end %>
</div>


